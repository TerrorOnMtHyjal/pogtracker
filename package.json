{
  "name": "pogtracker",
  "version": "1.0.0",
  "description": "A Twitch highlight generator based on chat activity.",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "heroku-postbuild": "cd client && npm run build",
    "dev": "run-p start:proxy start:server start:client",
    "dev:proxy": "babel-watch -w index.js",
    "dev:server": "babel-watch -w server/server.js"
  },
  "cacheDirectories": [
    "node_modules",
    "server/node_modules",
    "client/node_modules"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dustwise/pogtracker.git"
  },
  "author": "Jared Mohney",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dustwise/pogtracker/issues"
  },
  "homepage": "https://github.com/dustwise/pogtracker#readme",
  "dependencies": {
    "express": "^4.15.3",
    "http-proxy-middleware": "^0.17.4",
    "npm-run-all": "^4.0.2"
  },
  "devDependencies": {}
}
